{
  "name": "ngx-webstorage-service",
  "version": "4.1.0",
  "description": "Angular service wrappers for the webstorage API",
  "homepage": "https://github.com/dscheerens/ngx-webstorage-service",
  "author": "Daan Scheerens",
  "license": "MIT",
  "keywords": [
    "angular",
    "ng",
    "ngx",
    "webstorage",
    "storage",
    "localstorage",
    "sessionstorage",
    "local",
    "session"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/dscheerens/ngx-webstorage-service.git"
  },
  "scripts": {
    "ng": "ng",
    "lint": "tslint -p lib/tsconfig.lib.base.json -t stylish -c lib/tslint.json",
    "test": "ng test --project=lib --watch=false --code-coverage",
    "test.watch": "ng test --project=lib --watch=true --code-coverage --browsers=Chrome",
    "build": "ng build --project=lib --configuration production",
    "release": "standard-version --message \"release: v%s\"",
    "publish.dist": "npm publish build/dist"
  },
  "sideEffects": false,
  "peerDependencies": {
    "@angular/core": ">=13.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.1.2",
    "@angular/cli": "^13.1.2",
    "@angular/common": "^13.1.1",
    "@angular/compiler": "^13.1.1",
    "@angular/compiler-cli": "^13.1.1",
    "@angular/core": "^13.1.1",
    "@angular/platform-browser": "^13.1.1",
    "@angular/platform-browser-dynamic": "^13.1.1",
    "@dscheerens/tslint-presets": "^9.0.0",
    "@types/jasmine": "~3.10.2",
    "@types/jasminewd2": "~2.0.10",
    "codelyzer": "^6.0.2",
    "jasmine-core": "^3.10.1",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.3.9",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.1.0",
    "karma-jasmine": "^4.0.1",
    "ng-packagr": "^13.1.2",
    "rxjs": "^6.6.2",
    "standard-version": "^9.3.2",
    "tslib": "^2.3.0",
    "tslint": "~6.1.3",
    "typescript": "~4.4.4",
    "typescript-tslint-plugin": "^1.0.1",
    "zone.js": "~0.11.4"
  }
}
